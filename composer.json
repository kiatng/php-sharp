{
    "name": "kiatng/php-sharp",
    "description": "A PHP wrapper for sharp js",
    "license": "MIT",
    "authors": [
        {
            "name": "Ng Kiat Siong",
            "email": "kiatsiong.ng@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/process": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kiatng\\Sharp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiatng\\Sharp\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "keywords": [
        "sharp",
        "image",
        "processing",
        "resize",
        "optimize"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
